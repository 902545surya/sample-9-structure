#include<stdio.h>
struct employe
{
   char empname[20];
   int age;
   double phnum;
   int salary;
}s[20];
int main()
{

   int i;
   for(i=0;i<4;++i)
   {
   printf("enter the empname ");
   scanf("%s",&s[i].empname);
   printf("enter emp age");
   scanf("%d",&s[i].age);
   printf("enter emp ph num");
   scanf("%lf",&s[i].phnum);
   printf("enter emp salary");
   scanf("%d",&s[i].salary);
   }
   printf("\n");
   for(i=0;i<4;i++)
   {
printf("\n empname:");
puts(s[i].empname);
printf(" \t  emp age :%d",s[i].age);
printf("\t emp ph num :%lf",s[i].phnum);
printf("\t emp salary :%d",s[i].salary);
   }
return 0;
}

